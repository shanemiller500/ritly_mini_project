<h1 class="wow">Ritly</h1>

<div id="box" class="animated @keyframes bounceInRight">

  <% @urls.each do |il| %>
<li> <%= link_to il.link, url_path(il), :class =>"text-new1"  %>
  
 <%= link_to @url.random_string, go_path(il.random_string),:class => "text-new1" %> <br>
    <p class="text2"> Created at: <%=il.created_at %> </p><br>
    <p class="text-list"> Updated at: <%=il.updated_at %></p><br>
 <%= link_to "delete", il, method: :delete,:class => "btn btn-lg btn-danger", data:{confirm: "Are you sure" }  %> <br>

 <%= link_to "EDIT PAGE", edit_url_path(il[:id]), :class => "btn btn-lg btn-info"  %><br><br>
  <br> </li>
<%end  %>

 <%= link_to "HOME PAGE", root_path, :class => "btn btn-lg btn-danger"   %>

 </div>